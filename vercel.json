{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.11"
    },
    "functions": {
        "main.py": {
            "memory": 512,
            "maxDuration": 30
        }
    },
    "regions": [
        "cdg1"
    ]
}